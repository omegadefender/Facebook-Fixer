{
    "manifest_version": 2,
    "name": "Hide Facebook's 'People you may know'",
    "version": "0.1",
    "icons": { "128": "128.png",
                "48": "icon48.png" },

    "content_scripts": [
      {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": ["content.js"]
      }
    ]
  }